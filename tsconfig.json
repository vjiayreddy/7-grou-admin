{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "redux/**/*.ts",
    "components/**/*.ts",
    "**/*.ts",
    "**/**.ts",
    "**/*.tsx",
    "**/**.tsx",
    ".next/types/**/*.ts",
    "app/face/_analysisResults.jsx",
    "app/face/_imageCapture.js",
    "app/face/_progress.jsx",
    "app/face/_skinAnalizer.jsx", "app/biopassid/page.js",
  ],
  "exclude": [
    "node_modules"
  ]
}